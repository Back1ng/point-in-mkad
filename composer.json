{
  "name": "back1ng/point-in-mkad",
  "type": "library",
  "description": "Detect, if point in Moscow Ring Road (MKAD)",
  "keywords": ["mkad"],
  "authors": [
    {
      "name": "Anatoly Elyutin",
      "email": "anatolyu128@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "hannesvdvreken/pip": "^1.1",
    "mjaschen/phpgeo": "^4.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "infection/infection": "^0.26.16",
    "friendsofphp/php-cs-fixer": "^3.13",
    "phpstan/phpstan": "^1.8",
    "vimeo/psalm": "^4.29",
    "rector/rector": "^0.15.1"
  },
  "autoload": {
    "psr-4": {
      "Back1ng\\PointInMkad\\": "src/",
      "Back1ng\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "infection/extension-installer": true
    }
  }
}
